[bandwidth]
command=$SCRIPT_DIR/bandwidth/bandwidth
interval=5

[iface]
command=$SCRIPT_DIR/iface/iface
color=#009900
interval=10

[iface]
command=$SCRIPT_DIR/iface/iface
LABEL=tun0:
IFACE=tun0
color=#00FF00
interval=10

[load_average]
command=$SCRIPT_DIR/load_average/load_average
interval=10
label=LOAD

[memory]
command=$SCRIPT_DIR/memory/memory
label=MEM
interval=30

[time]
command=$SCRIPT_DIR/time/time
interval=5
STRFTIME_FORMAT=%Y-%m-%d %H:%M:%S
TZONES=$DEFAULT_TZ
TZ_LABELS=,
